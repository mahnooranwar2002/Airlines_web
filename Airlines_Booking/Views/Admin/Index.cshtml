 @model Airlines_Booking.Models.mainmodel;


@{
    Layout = "../shared/adminDashboard_layout.cshtml";
}
<div class="container-fluid">
    <div class="main-content">
        <h1>Dashbroad</h1>
        <div class="row">
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count">  @TempData["admin_count"]</div>
                        <div class="icon"><i class='bx bxs-user-check'></i></div>
                    </div>
                    <div class="card-footer">
                        Total Admin
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count"> @TempData["user_count"]</div>
                        <div class="icon">    <i class='bx bxs-user'></i></div>
                    </div>
                    <div class="card-footer">
                        Total User
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count">@TempData["plane_count"]</div>
                        <div class="icon">   <i class='bx bxs-plane-alt'></i></div>
                    </div>
                    <div class="card-footer">
                        Total Areoplane
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count"> @TempData["country_count"]</div>
                        <div class="icon"> <i class='bx bx-globe'></i></div>
                    </div>
                    <div class="card-footer">
                        Total Countries
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count">  @TempData["city_count"]</div>
                        <div class="icon">  <i class='bx bxs-location-plus'></i></div>
                    </div>
                    <div class="card-footer">
                        Total Cities
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count"> @TempData["user_count"]</div>
                        <div class="icon">    <i class='bx bxs-user'></i></div>
                    </div>
                    <div class="card-footer">
                        Total User
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count">@TempData["flight_count"]</div>
                        <div class="icon">
                            <i class='bx bxs-plane-take-off'></i>
                        </div>
                    </div>
                    <div class="card-footer">
                        Total Flights
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count"> @TempData["staff_count"]</div>
                        <div class="icon"> <i class='bx bxs-group'></i></div>
                    </div>
                    <div class="card-footer">
                        Total Staff
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count">  @TempData["package_count"]</div>
                        <div class="icon">   <i class='bx bxs-shopping-bag-alt'></i></div>
                    </div>
                    <div class="card-footer">
                        Total Packages
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count"> @TempData["contact_count"]</div>
                        <div class="icon">    <i class='bx bxs-phone'></i></div>
                    </div>
                    <div class="card-footer">
                        Total Contact
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count">@TempData["earning"]</div>
                        <div class="icon">   <i class='bx bxs-dollar-circle'></i></div>
                    </div>
                    <div class="card-footer">
                       Total Earning
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card card-count">
                    <div class="card-body">
                        <div class="count"> @TempData["seat"] </div>
                        <div class="icon"> <i class='bx bx-chair'></i></div>
                    </div>
                    <div class="card-footer">
                        Available Seats
                    </div>
                </div>
            </div>



















        </div>
    </div>
</div>











    
